<div class="panel panel-default">

	<div class="panel-heading">
		<h1>All Lists</h1>
	</div>

	<div class="panel-body">
		<% @lists.each do |list| %>
			<%= link_to list.name, list %> <br>
		<% end %>
	</div>
	
	<div class="panel-footer">
		<%= link_to	"Add a new list", new_list_path, class: "btn btn-primary" %>
	</div>
</div>

